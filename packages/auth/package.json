{
  "name": "@remindle/auth",
  "version": "1.0.0",
  "description": "React microfrontend auth",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "serve": "vite serve",
    "build": "vite build",
    "build:c": "concurrently \"vite build --watch\" \"vite preview\"",
    "preview": "vite build && vite preview",
    "start": "yarn dev & wait-on tcp:8080 -v",
    "clean": "rm -rf dist",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix",
    "type-check": "tsc"
  },
  "repository": "https://github.com/remindle/frontend/packages/auth",
  "author": "Leo van Haaren <leo@remindle.io>",
  "license": "MIT",
  "dependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.1.1"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.1.0",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "~5",
    "@typescript-eslint/parser": "~5",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "eslint": "~8.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "~11.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "~5.2",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "history": "^5.1.0",
    "postcss": "~8.4",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.1",
    "typescript": "^4.5.3",
    "vite": "^2.7.2",
    "vite-plugin-externals": "^0.3.1",
    "wait-on": "^6.0.0"
  },
  "pre-commit": "lint"
}
